{
  "name": "@bepalo/cache",
  "version": "1.0.3",
  "exports": {
    "./mod.ts": "./mod.ts"
  },
  "description": "A fast and modern in-memory cache library with TTL and LRU for JavaScript runtimes.",
  "license": "MIT",
  "homepage": "https://github.com/bepalo/cache",
  "repository": {
    "type": "git",
    "url": "https://github.com/bepalo/cache"
  },
  "compilerOptions": {
    "strict": true
  },
  "lint": {
    "include": ["./src/", "./bench/", "./tests/"],
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-sloppy-imports"]
    }
  },
  "fmt": {
    "include": ["./src/", "./bench/", "./tests/"],
    "lineWidth": 100,
    "indentWidth": 2,
    "singleQuote": false,
    "proseWrap": "preserve"
  },
  "test": {
    "files": {
      "exclude": ["tests/**", "**/*.vitest.ts"]
    }
  },
  "imports": {
    "@bepalo/cache": "./mod.ts"
  },
  "keywords": [
    "cache",
    "lru",
    "ttl",
    "memory cache",
    "in-memory",
    "fast cache",
    "javascript cache",
    "nodejs cache",
    "deno cache",
    "bun cache",
    "local cache",
    "data caching",
    "expiry",
    "eviction policy",
    "cache performance",
    "custom expiration",
    "cache utility",
    "cache store",
    "lightweight cache"
  ]
}
